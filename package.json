{
  "name": "pdf-combine",
  "version": "1.0.1",
  "description": "There are various tools in the npm that allow for combining pdf's this tool is limited to Mac & Linux but it should work on macs out-of-the-box and on linux by using poppler`s `pdfunite`. This tool is by no means well tested use on own risk!",
  "main": "combinePdfs.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "lint": "standard",
    "quick": "tap -- test/combinePdfs.js",
    "test": "standard && tap -- test/combinePdfs.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ryd0rz/pdf-combine.git"
  },
  "keywords": [
    "pdf"
  ],
  "author": "Martin Heidegger",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ryd0rz/pdf-combine/issues"
  },
  "homepage": "https://github.com/ryd0rz/pdf-combine#readme",
  "devDependencies": {
    "standard": "^7.1.1",
    "tap": "^5.7.2"
  },
  "dependencies": {
    "command-exists": "^1.0.1",
    "fs": "0.0.1-security"
  }
}
